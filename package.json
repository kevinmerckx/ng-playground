{
  "name": "ng-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build ng-playground && cp README.md LICENSE dist/ng-playground && node-sass projects/ng-playground/src/ng-playground.sass dist/ng-playground/ng-playground.css",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": "10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinmerckx/ng-playground.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.12",
    "@angular/common": "7.2.12",
    "@angular/compiler": "7.2.12",
    "@angular/core": "7.2.12",
    "@angular/forms": "7.2.12",
    "@angular/http": "7.2.12",
    "@angular/platform-browser": "7.2.12",
    "@angular/platform-browser-dynamic": "7.2.12",
    "@angular/router": "7.2.12",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "http-auth": "^3.2.3",
    "iwerk-angular-ui": "5.0.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.3",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.0.0",
    "codelyzer": "4.5.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^5.1.0",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "~5.1.2",
    "semantic-release": "^15.13.3",
    "ts-node": "~8.1.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "3.2.4"
  },
  "release": {
    "pkgRoot": "dist/ng-playground",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "CHANGELOG.md"
        ]
      }
    ],
    "publish": [
      "@semantic-release/npm"
    ]
  }
}
